package Arrays;

import java.util.Scanner;

public class SortArrayOf0s1s2s {

	public static void 
	
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		
		int n = scn.nextInt();
		
		int arr[] = new int[n];
		
		for(int i = 0; i < n; i++) {
			arr[i] = scn.nextInt();
		}
		
		sort012(arr, n);
		
		StringBuffer str = new StringBuffer();
		
		for(int i = 0; i < n; i++) {
			str.append(arr[i] + " ");
		}
		
		System.out.println(str);
		
		scn.close();
	}

}
